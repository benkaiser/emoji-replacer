{
  "manifest_version": 2,

  "name": "Emoji Replacer",
  "description": "Automatically replaces text you are currently writing with Emoji!",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["https://www.messenger.com/*"],
      "js": ["bower_components/jquery/dist/jquery.slim.min.js", "emoji_bindings.js", "emoji.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
